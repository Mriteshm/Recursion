import java.util.Scanner;

public class runner {
	public static int sumOfDigits(int input)
{
		if(input==0)
		{
			return 0;
		}
		   int a=input%10;
		 int b=  sumOfDigits(input/10);
		   return a=a+b;
	}
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		System.out.println(solution.sumOfDigits(n));
	}
}
